# API Keys
GOOGLE_API_KEY=your_google_api_key_here
# Có thể thêm nhiều API keys cách nhau bằng dấu phẩy

# Qdrant Vector Store Configuration
QDRANT_URL=your_qdrant_url_here
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_COLLECTION_NAME=your_collection_name

# Search Tool Configuration
GOOGLE_API_KEY=your_google_api_key_here
SERPER_API_KEY=your_serper_api_key_here

# Embedding Model Configuration
EMBEDDING_MODEL_NAME=intfloat/multilingual-e5-base
#EMBEDDING_MODEL_NAME=paraphrase-multilingual-mpnet-base-v2

# LLM Configuration
LLM_MODEL_NAME=gemini-2.0-flash
LLM_TEMPERATURE=0
LLM_TOP_P=0.85
LLM_MAX_OUTPUT_TOKENS=2048 

# API Configuration
API_PREFIX=/api
API_TITLE="Hệ thống RAG cho Cơ sở dữ liệu"
API_DESCRIPTION="API cho hệ thống tìm kiếm và trả lời câu hỏi sử dụng RAG"
API_VERSION="1.0.0"

# Search Configuration
RERANKER_MODEL=cross-encoder/mmarco-mMiniLMv2-L12-H384-v1

# Parallel Processing Configuration
MAX_PARALLEL_WORKERS=4

# Fact Checking Configuration
ENABLE_FACT_CHECKING=true
FACT_CHECKING_THRESHOLD=0.6

# Cấu hình hybrid search
# Tăng DEFAULT_ALPHA cho người dùng muốn ưu tiên tìm kiếm ngữ nghĩa
# Giảm DEFAULT_ALPHA cho người dùng muốn ưu tiên tìm kiếm từ khóa
DEFAULT_ALPHA=0.7

# Cấu hình xử lý tài liệu
# Giảm CHUNK_SIZE sẽ tạo ra nhiều đoạn văn nhỏ hơn nhưng chính xác hơn
# Tăng CHUNK_OVERLAP giúp bảo toàn ngữ cảnh giữa các đoạn
CHUNK_SIZE=800
CHUNK_OVERLAP=200

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Conversation Storage Configuration
USE_SUPABASE_CONVERSATIONS=True